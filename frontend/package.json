{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
     "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:validate": "next build && npm run type-check",
    "build:analyze": "ANALYZE=true npm run build",
    "type-check": "tsc --noEmit",
    "lint:strict": "next lint --strict",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .next && rimraf node_modules/.cache",
    "clean:all": "rimraf .next && rimraf node_modules && npm install",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "preview": "npm run build && npm run start",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "deps:update": "npm update && npm audit",
    "deps:audit": "npm audit --audit-level moderate",
    "deps:check": "npx npm-check-updates",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "security:audit": "npm audit --audit-level moderate",
    "security:check": "npx audit-ci --moderate",
    "performance:analyze": "ANALYZE=true npm run build",
    "performance:lighthouse": "npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "cache:clear": "rimraf .next && npm run dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-query-devtools": "^5.51.23",
    "@types/crypto-js": "^4.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.12.1",
    "gsap": "^3.13.0",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "tailwind-merge": "^3.3.0",
    "yup": "^1.6.1",
    "zod": "^3.25.28",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.2",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/nextjs": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitest/coverage-v8": "^2.1.5",
    "@vitest/ui": "^2.1.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-storybook": "^0.11.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "storybook": "^8.4.7",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.5"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
